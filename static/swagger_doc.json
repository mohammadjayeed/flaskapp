          "address": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "country": {
            "type": "string"
          }
        }
      },
      "SimpleRole": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "active": {
            "type": "boolean"
          },
          "company": {
            "type": "string"
          },
          "sex": {
            "type": "string"
          },
          "contact": {
            "$ref": "#/components/schemas/SimpleContact"
          },
          "role": {
            "$ref": "#/components/schemas/SimpleRole"
          }
        },
        "required": [
          "company",
          "first_name",
          "last_name",
          "sex"
        ]
      },
      "UserPostUpdateSchemaJSON": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "active": {
            "type": "boolean"
          },
          "company": {
            "type": "string"
          },
          "sex": {
            "type": "string"
          }
        },
        "required": [
          "active",
          "company",
          "first_name",
          "last_name",
          "sex"
        ]
      },
      "ContactCreate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "phone": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "country": {
            "type": "string"
          }
        },
        "required": [
          "address",
          "city",
          "country",
          "phone"
        ]
      },
      "RoleCreate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "name"
        ]
      }
    },
    "responses": {
      "UNPROCESSABLE_ENTITY": {
        "description": "Unprocessable Entity",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      },
      "DEFAULT_ERROR": {
        "description": "Default error response",
        "content": {
